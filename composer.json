{
    "name": "bonnier/wp-bonnier-cache",
    "description": "Integrates Bonnier Caching into WordPress",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Bonnier Publications",
            "email": "interactive@bonnier.dk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type":"vcs",
            "url":"https://github.com/junaidbhura/composer-wp-pro-plugins"
        },
        {
            "type": "package",
            "package": {
                "name": "junaidbhura/advanced-custom-fields-pro",
                "version": "5.7.2",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://www.advancedcustomfields.com"
                },
                "require": {
                    "junaidbhura/composer-wp-pro-plugins": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "junaidbhura/polylang-pro",
                "version": "2.3.8",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://www.polylang.pro"
                },
                "require": {
                    "junaidbhura/composer-wp-pro-plugins": "*"
                }
            }
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "bonnier/contenthub-editor": "^4.0",
        "bonnier/willow-mu-plugins": "^1.1"
    },
    "require-dev": {
        "johnpbloch/wordpress": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Bonnier\\WP\\Cache\\": "src"
        }
    }
}
